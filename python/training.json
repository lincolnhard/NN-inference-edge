{
    "name": "FCOSTrainer",
    "num_epochs": 1000,
    "use_cuda": true,
    "parallel": true,
    "reset_optimizer": false,
    "model": {
        "name": "MnasNetA1FCOS",
        "num_classes": 7,
        "width_mult": 1.0,
        "feature_layers": [6, 12, 15],
        "strides": [8, 16, 32, 64, 128],
        "mod_fpn_rpn": true,
        "sa": true,
        "max_joints": 6,
        "use_l2norm": false
    },
    "criterion": {
        "name": "FCOSLoss",
        "strides": [8, 16, 32, 64, 128]
    },
    "dataset": {
        "name": "fcos_detection_dataloaders",
        "root": "/notebook/dataset/0319",
        "batch_size": 32,
        "size": [384, 640],
        "labels": ["sign4", "sign4y", "sign3", "sign3rev", "sign6", "dlane-WH4"],
        "joints": [4, 4, 3, 3, 6, 4],
        "num_workers": 16,
        "mean": [0.3856, 0.4016, 0.4108],
        "std": [0.2572, 0.2660, 0.2810]
    },
    "optimizer": {
        "name": "SGD",
        "lr": 1e-2,
        "momentum": 0.9,
        "weight_decay": 1e-4,
        "nesterov": true
    },
    "scheduler": {
        "name": "CosineAnnealingLR",
        "T_max": 1000,
        "eta_min": 1e-5
    }
}
